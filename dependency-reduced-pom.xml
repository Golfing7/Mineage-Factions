<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.massivecraft.factions</groupId>
  <artifactId>Factions</artifactId>
  <name>${project.artifactId}</name>
  <version>2.18.0</version>
  <build>
    <resources>
      <resource>
        <filtering>true</filtering>
        <directory>${project.basedir}</directory>
        <includes>
          <include>*.yml</include>
        </includes>
      </resource>
    </resources>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
          <compileSourceRoots>src</compileSourceRoots>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>3.5.0</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration>
              <minimizeJar>false</minimizeJar>
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
  <repositories>
    <repository>
      <id>ess-repo</id>
      <url>http://repo.ess3.net/content/groups/essentials</url>
    </repository>
    <repository>
      <id>essentials-releases</id>
      <url>https://repo.essentialsx.net/releases/</url>
    </repository>
    <repository>
      <id>paper-repo</id>
      <url>https://repo.destroystokyo.com/repository/maven-public/</url>
    </repository>
    <repository>
      <id>polar-repo</id>
      <url>https://repo.polar.top/repository/polar/</url>
    </repository>
    <repository>
      <id>jitpack.io</id>
      <url>https://jitpack.io</url>
    </repository>
  </repositories>
  <dependencies>
    <dependency>
      <groupId>io.papermc.paper</groupId>
      <artifactId>paper-api</artifactId>
      <version>1.17-R0.1-SNAPSHOT</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>top.polar</groupId>
      <artifactId>api</artifactId>
      <version>2.0.0</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>net.coreprotect</groupId>
      <artifactId>CoreProtect</artifactId>
      <version>LATEST</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/libs/CoreProtect.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>net.techcable.tacospigot</groupId>
      <artifactId>WineSpigot</artifactId>
      <version>1.8.8-R0.2-SNAPSHOT</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/libs/WineSpigot.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>testbuild</groupId>
      <artifactId>testbuild</artifactId>
      <version>LATEST</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/libs/craftbukkit-1.8.8.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.massivecraft</groupId>
      <artifactId>MassiveCore</artifactId>
      <version>LATEST</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/libs/MassiveCore.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.sk89q.worldedit</groupId>
      <artifactId>worldedit</artifactId>
      <version>LATEST</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/libs/worldedit.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.sk89q.worldguard</groupId>
      <artifactId>worldguard-legacy</artifactId>
      <version>LATEST</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/libs/worldguard-6.0.0-beta-04.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>net.milkbowl.vault</groupId>
      <artifactId>Vault</artifactId>
      <version>1</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/libs/Vault.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>net.essentialsx</groupId>
      <artifactId>EssentialsX</artifactId>
      <version>2.21.0</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/libs/EssentialsX-2.21.0.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>org.jdgames</groupId>
      <artifactId>SilkSpawners</artifactId>
      <version>LATEST</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/libs/SilkSpawners.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>org.jdgames</groupId>
      <artifactId>Citizens</artifactId>
      <version>LATEST</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/libs/Citizens-2.0.26-b1996-FIXED.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>Fkore</groupId>
      <artifactId>FKore</artifactId>
      <version>LATEST</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/libs/FactionsKore-1.3.51.1.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>SupplyDrop</groupId>
      <artifactId>SupplyDrop</artifactId>
      <version>LATEST</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/libs/SupplyDrops-v1.1.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.github.N0RSKA</groupId>
      <artifactId>UltimateAutoRestartAPI</artifactId>
      <version>2023.12</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/libs/UltimateAutoRestart-2023.12.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>net.minelink.ctplus</groupId>
      <artifactId>CombatTagPlus</artifactId>
      <version>1.0.0</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/libs/CombatTagPlus-1.0.0-SNAPSHOT.jar</systemPath>
    </dependency>
  </dependencies>
</project>
